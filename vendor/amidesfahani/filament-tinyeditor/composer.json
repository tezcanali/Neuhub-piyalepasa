{
    "name": "amidesfahani/filament-tinyeditor",
    "description": "Filament TinyMce Editor v6",
    "license": "MIT",
    "keywords": ["laravel", "filament", "tinyeditor", "tinymce"],
    "authors": [
        {
            "name": "Amid Esfahani",
            "email": "amid.esfahani@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.16",
        "tinymce/tinymce": "^7.3.0"
    },
    "autoload": {
        "psr-4": {
            "AmidEsfahani\\FilamentTinyEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmidEsfahani\\FilamentTinyEditor\\TinyeditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.13"
    }
}
